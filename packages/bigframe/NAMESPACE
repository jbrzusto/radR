S3method("$<-", "bigframe")
S3method("close", "bigframe")
S3method("dim", "bigframe")
S3method("dim<-", "bigframe")
S3method("flush", "bigframe")
S3method("names", "bigframe")
S3method("print", "bigframe")
S3method("rbind", "bigframe")
