useDynLib("extmat", .registration=TRUE)
export(extmat)
export(name)
export("name<-")
export(zero)
export(pointer)
S3method("print", "extmat")
S3method("as.character", "extmat")
S3method("as.vector", "extmat")
S3method("dim", "extmat")
S3method("dim<-", "extmat")
S3method("format", "extmat")
S3method("length", "extmat")
S3method("length<-", "extmat")
S3method("[", "extmat")
S3method("[<-", "extmat")
S3method("name", "extmat")
S3method("name<-", "extmat")
S3method("pointer", "extmat")
S3method("zero", "extmat")
